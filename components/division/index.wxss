.division {
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: center;
  padding: 20rpx;
}
.right-wrapper {
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: center;
  margin-top: 6rpx;
}
.left-title {
  display: flex;
  flex: 1;
  font-size: 40rpx;
  font-weight: bold;
}
.right-title {
  font-size: 26rpx;
  margin: 0 4rpx 4rpx 10rpx;
  color: #808080;
}
.refresh {
  display: flex;
  flex-direction: row;
  align-items: center;
  /* margin-right:10rpx; */
  margin-top: 6rpx;
}
.line {
  height: 30rpx;
  width: 1px;
  background-color: #808080;
  margin-left: 6rpx;
}
